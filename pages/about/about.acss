.box {
    width: 100%;
    height: 100%;
}

.titlecss {
  border-top: 2rpx solid #e9e9e9;
  border-bottom: 2rpx solid #e9e9e9;
  height: 360rpx;
  width: 100%;
  background-color: #fff;
  display: flex;
  align-items: center;
  flex-direction: column;
}

.imgcontent {
  width: 188rpx;
  height: 188rpx;
  background-color: #f7f7f7;
  border-radius: 19rpx;
  margin-top: 52rpx;
}

.iconimg {
  width: 100%;
  height: 100%;
  background-size: 90% 90%;
  background-position: center;
  background-repeat: no-repeat;
  background-image: url('data:image/png;base64,');
}

.titletext {
  margin-top: 18rpx;
  font-size: 32rpx;
}

.contentcss {
  border-top: 2rpx solid #e9e9e9;
  border-bottom: 2rpx solid #e9e9e9;
  width: 100%;
  background-color: #fff;
  margin-top: 22rpx;
}

.content {
  display: flex;
  width: 100%;
  height: 105rpx;
  font-size: 32rpx;
  padding-left: 38rpx;
  align-items: center;
  justify-content: space-between;
}

.righticon {
  margin: 0 80rpx 0 0;
  width: 14rpx;
  height: 24rpx;
}

.line {
  width: 100%;
  height: 1rpx;
  border-bottom: 1rpx solid #e9e9e9;
  margin-left: 38rpx;
}
