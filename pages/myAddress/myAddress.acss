.wrapbox{
    position: relative;
}
.box {
   background: #F2F2F2;
   width: 750rpx;
}
/* tab切换 */
.box .tabs{
    background: #ffffff;
    width:750rpx;
    height:104rpx;
    line-height: 104rpx;
    color: #121212;
    font-size: 32rpx;
    display: flex;

}
.box .tabs .tab{
    width:250rpx;
    text-align: center;
}
.box .tabsline{
    position: relative;
    left:0rpx;
    width: 250rpx;
    height: 3rpx;
    background: #F45322;
    margin:0 0 16rpx 0;
    transition:0.4s all ease;
}
.box .tabscontents .tabcontent .li{
    margin:0 0 20rpx 0;
    background: #ffffff;
}
.box .tabscontents .tabcontent{
    display: none;
}
.box .tabscontents .tabcontent.show{
    display: block;
    padding: 10rpx;
}
.box .tabscontents .tabcontent.hidden{
    display: none;
}

.wrap {
    position: relative;
}

.wrap::-webkit-scrollbar {
    display: none;
}

.search-wrap {
    display: flex;
    padding: 18rpx 0 26rpx 42rpx;
}

.search-ipt {
    display: flex;
    position: relative;
    /* width: 560rpx; */
    width: 520rpx;
    height: 72rpx;
    border-radius: 36rpx;
    border: 1rpx solid #D9D9D9;
    background: #fff;
}

.search-ipt .ipt-icon {
    line-height: 72rpx;
    margin-right: 22rpx;
    margin-left: 30rpx;
}
.search-ipt .ipt-icon image{
    width: 48rpx;
    height: 50rpx;
    margin: 11rpx 0;
    background-size: contain;
}

.form1 {
    display: flex;
    width: 100%;
    height: 100%;
    align-items: center;
    justify-content: center;
}

.ipt-wrap {
    width: 100%;
}

.ipt-wrap input {
    display: block;
    border: none;
    background: none;
    width: 90%;
    height: 65rpx;
    line-height: 65rpx;
    font-size: 28rpx;
}

.address-manage {
    flex: 1;
    text-align: center;
    height: 72rpx;
    line-height: 72rpx;
    font-size: 32rpx;
}
.address-manage1 {
    flex: 1;
    text-align: center;
    height: 72rpx;
    line-height: 72rpx;
    font-size: 32rpx;
    background-repeat: no-repeat;
    background-position: center;
    background-size: 60% 60%;
    background-image: url('data:image/png;base64,')
}
.address-manage.active{
    color: #E83F57;
}



.content-wrap{
    border-bottom:2rpx solid #E9E9E9;
}

.content-wrap {
    padding: 30rpx 40rpx 14rpx 42rpx;
}
.adr-wrap {
    display: flex;
    margin-bottom: 5rpx;
}

.adr-wrap  view {
    width: 50%;
    color: #121212;
}

.adr-name {
    font-size: 36rpx;
    overflow: hidden;
    height: 40rpx;
    text-overflow: ellipsis;
    white-space: nowrap;
        -webkit-line-clamp: 1;
}

.adr-phone {
    font-size: 32rpx;
    text-align: right;
}

.adr-detail {
    font-size: 28rpx;
    color: #898989;
    line-height: 40rpx;
    word-break: break-all;
        width: 100%;
    overflow: hidden;
}

.adr-edit {
    position: relative;
    display: flex;
    height: 78rpx;
    line-height: 78rpx;
    padding: 0 40rpx 0 42rpx;
}
.adr-edit .image{
    width: 36rpx;
    height:36rpx;
    margin: 21rpx 10rpx 21rpx 0;
    background-size: contain;
}

.edit-btn, .detail-btn {
    position: absolute;
    top: 0;
    display: flex;
    font-size: 28rpx;
    color: #898989;
}

.edit-btn .iconfont, .detail-btn .iconfont {
    display: block;
    font-size: 40rpx;
    margin-right: 16rpx;
}

.edit-btn {
    right: 206rpx;
}

.detail-btn {
    right: 42rpx;
}

.new-add-address {
    position: fixed;
    left: 0;
    bottom: 58rpx;
    width: 100%;
    z-index: 999;
}
.new-add-address.showBtn{
    display: block;
}

.new-add-address button {
    display: block;
    margin: 0 auto;
    width: 600rpx;
    height: 88rpx;
    line-height: 88rpx;
    border: none;
    border-radius: 60rpx;
    background-color: #E83F57;
    font-size: 32rpx;
    color: #fff;
    box-shadow: -1rpx 2rpx 15rpx #280909;
}

input[type=search]::-webkit-search-cancel-button {
    -webkit-appearance: none;
}

.loading{
    position: fixed;
    left:50%;
    top:40%;
    transform: translate(-50%);
}
.loading image{
    width: 100rpx;
    height:100rpx;
    background-size: contain;
}
.noaddress{
    position: absolute;
    left:50%;
    top:45%;
    transform: translate(-50%);
}
.noaddress image{
    width: 250rpx;
    height:286rpx;
    background-size: contain;
}
.noaddress .addresstxt{
    margin:34rpx 0 0 0;
    font-size: 24rpx;
    color: #797979;
}
.noaddress .addaddress{
    margin:54rpx auto 0;
    font-size: 28rpx;
    color: #E83F57;
    width: 166rpx;
    height: 56rpx;
    line-height: 56rpx;
    border:1px solid #E83F57;
    border-radius: 20px;
    text-align: center;

}

.dotradio .a-image{
    top:0!important;
}
.tabscontents{

}
.sent-address{
   
}
.sent-address.fain{
    height:auto;
    margin: 0；
}

.sent-address.opacity{
    opacity: 1;
}
.zhifubao-btn image{
    width: 100%;
}
.zhifubao-btn{
    margin-bottom: 10rpx;
}
.scroll{
    padding-bottom: 120rpx;
    box-sizing: border-box;
}

@keyframes shadow{
  0%{
    box-shadow:0rpx 0rpx 20rpx rgb(76,120,232);
  }
  100%{
    box-shadow:0rpx 0rpx 0rpx rgb(76,120,232);
  }
}

.zhifubao-address:first-child{
  animation: shadow 1.5s ease-in 0.3s;
}
